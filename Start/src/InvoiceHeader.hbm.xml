<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping SYSTEM 
"hibernate-mapping-3.0.dtd">


<hibernate-mapping>
	<class name="org.demo.spinncast.hibernate.InvoiceHeaderHBC" table="inv_hdr">
		<id name="invId" column="inv_id">
			<generator class="native"/>
		</id>
		<property name="invDate" column="inv_date"></property>
		<property name="customerId" column="customer_id"></property>
		<property name="purchaseOrderId" column="purchase_order_no"></property>
		<property name="purchaseOrderDate" column="purchase_order_date"></property>
		<property name="modeOfTransport" column="mode_of_transport"></property>
		<property name="vehicleNo" column="vehicle_no"></property>
		<property name="netTotalAmount" column="net_total_amount"></property>
		<property name="freightInsurance" column="freight_insurance"></property>
		<property name="grandTotal" column="grand_total"></property>
		<property name="tcNo" column="tcNo"></property>
		<property name="irNo" column="irNo"></property>
		<property name="lrNo" column="lrNo"></property>
		<property name="paymentTerms" column="paymentTerms"></property>
		<property name="pkgFrwdChg" column="pkg_frwd_chg"></property>
		<property name="invIssueDate" column="inv_issue_date"></property>
		<property name="removalDate" column="removal_date"></property>
		<property name="liAmountTotal" column="li_amount_total"></property>
		<property name="bedRate" column="bed_rate"></property>
		<property name="edCessRate" column="ed_cess_rate"></property>
		<property name="shsCess" column="shs_cess"></property>
		<property name="vatOrCst" column="vat_or_cst"></property>
	</class>
</hibernate-mapping>  