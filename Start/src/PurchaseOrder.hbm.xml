<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping SYSTEM 
"hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.demo.spinncast.hibernate.PurchaseOrderHBC" table="purchase_order">
		<id name="purchaseOrderId" column="purchase_order_id">
			<generator class="native"/>
		</id>
		<property name="purchaseOrderNo" column="purchase_order_no"></property>
		<property name="customerId" column="customer_id"></property>
		<property name="purchaseOrderDate" column="purchase_order_date"></property>
	</class>
</hibernate-mapping>  

