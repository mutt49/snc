<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping SYSTEM 
"hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.demo.spinncast.hibernate.PurchaseOrderHBC" table="purchase_order">
		<id name="order_id" column="order_id">
			<generator class="native"/>
		</id>
		<property name="description" column="description"></property>
		<property name="gross_price" column="gross_price"></property>
		<property name="net_value" column="net_value"></property>
	</class>
</hibernate-mapping>  